<Window x:Class="CommandBindingDemo.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CommandBindingDemo"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Button Height="23" Width="75" Margin="22 21 0 0" HorizontalAlignment="Left" VerticalAlignment="Top" Command="ApplicationCommands.Copy" >_Copy</Button>
        <Button Height="23" Width="75" Margin="22 61 0 0"  HorizontalAlignment="Left" VerticalAlignment="Top" Command="ApplicationCommands.Paste" >Paste</Button>
    </Grid>
</Window>
